<h1><center><u>Player Search</u></center></h1>
<center><form method="get"><input type="text" value="" name="q" id="q" /></form></center>
<table>
<% @player.each do |player| %>
  <tr class="livesearch">
    <td>(<%=  player.rank %>)<%= player.first_name + " " + player.last_name %></td>
    <% if params[:lock] %> 
    <td><%= link_to 'Edit', edit_player_path(player) %></td>
    <%end%>
  </tr>
  </br>
<% end %>
</table>

<% javascript_tag do %>

Event.observe(window, 'load', function() { 
  new QuicksilverLiveSearch('q', '.livesearch');
});

<% end %>
