<% @teams.each do |team| %>
  <fieldset class="grid_5 column">
    <legend><span style="font-size:16px;"><%= team.name %></span>  <span style="color:#777777;font-size:10px;">(<%= team.player_name %>)</span></legend>
    <ul>
    <% team.players.each do |player| %>
      <li style="font-size:14px;"><%= player.first_name + " " + player.last_name %></li>   
    <%end%>
   </ul> 
</fieldset>
<% end %>

<br />
<% if params[:lock] %>
<%= link_to 'New Team', new_team_path %>
<% end %>
